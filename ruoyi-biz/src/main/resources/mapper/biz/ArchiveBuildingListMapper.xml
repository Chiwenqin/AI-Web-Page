<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.biz.mapper.ArchiveBuildingListMapper">

    <resultMap type="ArchiveBuildingList" id="ArchiveBuildingListResult">
            <result property="id" column="id"/>
            <result property="buildingCode" column="building_code"/>
            <result property="buildingName" column="building_name"/>
            <result property="sort" column="sort"/>
            <result property="entranceCount" column="entrance_count"/>
            <result property="houseCount" column="house_count"/>
            <result property="buildingArea" column="building_area"/>
            <result property="billingArea" column="billing_area"/>
            <result property="status" column="status"/>
            <result property="villageId" column="village_id"/>
            <result property="villageName" column="village_name"/>
            <result property="createPerson" column="create_person"/>
            <result property="updatePerson" column="update_person"/>
    </resultMap>

    <select id="selectListPage" resultMap="ArchiveBuildingListResult">
        SELECT
            ab.id,
            ab.building_code,
            ab.building_name,
            ab.sort,
            ab.entrance_count,
            ab.house_count,
            ab.building_area,
            ab.billing_area,
            ab.status,
            ab.village_id,
            av.village_name,
            ab.create_person,
            ab.update_person
        FROM archive_building_list ab
        LEFT JOIN archive_village_list av ON ab.village_id = av.id
        ${ew.customSqlSegment}
    </select>
</mapper>